<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tik_Tok">

    <Button
        android:id="@+id/b"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/b6"
        android:layout_alignTop="@+id/b5"
        android:textSize="30dp" />

    <Button
        android:id="@+id/b6"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:textSize="30dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/b3"
        android:layout_marginEnd="193dp" />

    <Button
        android:id="@+id/b11"
        android:layout_width="152dp"
        android:layout_height="90dp"
        android:textSize="30dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/b8"
        android:layout_marginEnd="24dp"
        android:text="RESET" />

    <Button
        android:id="@+id/b10"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:textSize="30dp"
        android:layout_alignStart="@+id/b"
        android:layout_alignTop="@+id/b8" />

    <Button
        android:id="@+id/b9"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:textSize="30dp"
        android:layout_alignStart="@+id/b3"
        android:layout_alignTop="@+id/b8" />

    <Button
        android:id="@+id/b8"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/b2"
        android:layout_marginBottom="14dp"
        android:textSize="30dp" />

    <Button
        android:id="@+id/b3"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/b5"
        android:layout_alignTop="@+id/b2"
        android:textSize="30dp" />

    <Button
        android:id="@+id/b5"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/b4"
        android:layout_marginStart="153dp"
        android:textSize="30dp" />

    <Button
        android:id="@+id/b2"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:layout_above="@+id/b11"
        android:layout_alignStart="@+id/b4"
        android:textSize="30dp" />

    <Button
        android:id="@+id/b4"
        android:layout_width="116dp"
        android:layout_height="90dp"
        android:textSize="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="84dp"
        android:layout_height="31dp"
        android:layout_alignStart="@+id/textView2"
        android:layout_centerVertical="true"
        android:text="Player two" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="84dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/b11"
        android:layout_marginTop="48dp"
        android:text="Player one" />

    <TextView
        android:id="@+id/t5"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/t4"
        android:textSize="20dp"
        android:textColor="@color/colorAccent"
        android:layout_centerVertical="true" />

    <TextView
        android:id="@+id/t4"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:textSize="20dp"
        android:textColor="@color/colorAccent"
        android:layout_alignTop="@+id/textView2"
        android:layout_marginEnd="15dp" />
</RelativeLayout>